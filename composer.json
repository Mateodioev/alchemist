{
    "name": "mateodioev/alchemist",
    "description": "Alchemist is a library for developing telegram bots, it supports inline queries (if the bot has it enabled), normal messages, and calbacks.",
    "keywords": ["telegram", "bot", "inline", "query", "callback", "alchemist", "alchemist-ace"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mateodioev\\Alchemist\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "vlucas/phpdotenv": "^5.4",
        "php-parallel-lint/php-console-color": "^1.0",
        "guzzlehttp/guzzle": "^7.4",
        "mateodioev/request": "dev-master",
        "mateodioev/db": "dev-master",
        "mateodioev/translate": "dev-master"
    },
    "authors": [
        {
            "name": "Mateodioev"
        }
    ]
}
